[user]
	name = Tyler Harms
	email = tyler.harms@gmail.com
[core]
	autocrlf = input
	safecrlf = true
[alias]
	co = checkout
	ci = commit
	st = status -sb
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[mergetool]
	keepBackup = true
[color]
	ui = 1
[push]
	default = simple
